<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <record id="custom_account_move" model="ir.sequence">
            <field name="name">Custom Account Move</field>
            <field name="code">custom.account.move</field>
            <field name="padding">3</field>
        </record>
    </data>

    <record id="view_order_form_account_move_inherit" model="ir.ui.view">
        <field name="name">account move inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="pan_no" invisible="1"/>
                <field name="msme" invisible="1"/>
            </xpath>
        </field>
    </record>

    <record id="view_account_move_search_inherited" model="ir.ui.view">
        <field name="name">account.move.search.inherit</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_account_invoice_filter"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="pan_no"/>
                <field name="msme"/>
            </xpath>
        </field>
    </record>
</odoo>