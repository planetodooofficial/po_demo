<odoo>
    <record forcecreate="True" id="ir_cron_hr_attendance" model="ir.cron">
        <field name="name">HR attendance: cron</field>
        <field name="model_id" ref="hr_attendance.model_hr_attendance"/>
        <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 23:59:59')"/>
        <field name="state">code</field>
        <field name="code">model.check_out_assign()</field>
        <field eval="True" name="active"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
    </record>

</odoo>