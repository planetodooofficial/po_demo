<odoo>
    <data noupdate="0">
        <record id="employees_access_roles" model="ir.rule">
            <field name="name">Employees to own records</field>
            <field name="model_id" ref="model_hr_employee"/>
            <field name="domain_force">[('user_id','=',user.id)]</field>
            <field name="groups" eval="[(4,ref('hr.group_hr_user'))]"/>
        </record>
        <record id="employees_all_access_role" model="ir.rule">
            <field name="name">Employees to Admin and  records</field>
            <field name="model_id" ref="model_hr_employee"/>
            <field name="domain_force">[]</field>
            <field name="groups" eval="[(4,ref('se_hr.group_HR_hod_user')),(4,ref('hr_attendance.group_hr_attendance_manager'))]"/>
        </record>
        <record id="employees_public_access_records" model="ir.rule">
            <field name="name">Public Employees to own records</field>
            <field name="model_id" ref="hr.model_hr_employee_public"/>
            <field name="domain_force">[('user_id','=',user.id)]</field>
            <field name="groups" eval="[(4,ref('hr.group_hr_user'))]"/>
        </record>
        <record id="employees_public_all_access_records" model="ir.rule">
            <field name="name">Public Employees to Admin and Hod</field>
            <field name="model_id" ref="hr.model_hr_employee_public"/>
            <field name="domain_force">[]</field>
            <field name="groups" eval="[(4,ref('se_hr.group_HR_hod_user')),(4,ref('hr_attendance.group_hr_attendance_manager'))]"/>
        </record>
    </data>
</odoo>