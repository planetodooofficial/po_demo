<odoo>
    <record forcecreate="True" id="ir_cron_hr_attendance" model="ir.cron">
        <field name="name">Quarter Calculation: cron</field>
        <field name="model_id" ref="se_appraisal.model_quarter_master"/>
        <field name="nextcall">2021-04-01 00:00:00</field>
<!--        <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 23:59:59')"/>-->
        <field name="state">code</field>
        <field name="code">model.quarter_assign()</field>
        <field eval="True" name="active"/>
        <field name="interval_number">12</field>
        <field name="interval_type">months</field>
        <field name="numbercall">-1</field>
    </record>

</odoo>